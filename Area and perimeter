#include <stdio.h>

int main()
{
    int l,w;
    float area,peri;
    
    printf("enter length\n");
    scanf("%d",&l);
    printf("enter width\n");
    scanf("%d",&w);         
    area=l*w;           
    peri=2*(l+w);       
    printf("Area: %f\n",area);
    printf("Perimeter: %f",peri);
    
    return 0;
}

