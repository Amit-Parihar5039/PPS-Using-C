include <stdio.h>
int main () 
  {
  int i,j,c[3][3];
  int a[3][3]={{1,2,3},{4,5,6},{1,2,3}};
  int b[3][3]={{1,2,3},{2,3,4},{1,2,3}};
  for(i=0;i<3;i++)
   {
   for(j=0;j<3;j++)
   {
     c[i][j]=a[i][j]+b[i][j];
     printf ("\t%d",c[i][j]);
      
       }
       printf ("\n");
       
      }
      return 0;
      }
